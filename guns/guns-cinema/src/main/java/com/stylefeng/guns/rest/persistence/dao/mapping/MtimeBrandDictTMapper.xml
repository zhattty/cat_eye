<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.stylefeng.guns.rest.persistence.dao.MtimeBrandDictTMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.stylefeng.guns.rest.persistence.model.MtimeBrandDictT">
        <id column="UUID" property="uuid" />
        <result column="show_name" property="showName" />
    </resultMap>
    <resultMap id="BaseResultMap2" type="com.stylefeng.guns.rest.persistence.model.responseVo.cinemaResponseVo.BrandVO">
        <id column="UUID" property="brandId" />
        <result column="show_name" property="brandName" />
    </resultMap>


    <select id="selectByBrandId" resultMap="BaseResultMap2">
        select UUID,show_name from mtime_brand_dict_t
        <where>
            <if test="brandId != 99">
                UUID=#{brandId}
            </if>
        </where>
    </select>
</mapper>
